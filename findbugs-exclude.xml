<?xml version="1.0" encoding="UTF-8"?>

<FindBugsFilter>

  <Match>
    <Bug pattern="RV_RETURN_VALUE_IGNORED_NO_SIDE_EFFECT"/>
  </Match>

  <Match>
    <!-- It's OK to return byte arrays here -->
    <Bug pattern="EI_EXPOSE_REP"/>
    <Or>
      <Class name="com.exonum.binding.storage.indices.MapEntry"/>
      <Class name="com.exonum.binding.storage.indices.ValueSetIndexProxy$Entry"/>
    </Or>
  </Match>
</FindBugsFilter>
