{
  "name": "exonum-java-binding-cryptocurrency-demo",
  "version": "1.0.0",
  "description": "Cryptocurrency demo for java binding",
  "author": "Exonum Team <exonum@bitfury.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/exonum/exonum-java-binding"
  },
  "scripts": {
    "proto": "pbjs --keep-case -t static-module ../src/main/proto/transactions.proto ../src/main/proto/wallet.proto -o ./src/proto/proto.js",
    "lint": "eslint --ext .js,.vue src test",
    "compile": "webpack",
    "test": "jest",
    "build": "npm run proto && npm run lint && npm run test && npm run compile",
    "start": "node server.js"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "big-integer": "^1.6.32",
    "bootstrap": "^4.3.1",
    "exonum-client": "^0.16.4",
    "jquery": "^3.4.0",
    "noty": "^3.2.0-beta",
    "numeral": "^2.0.6",
    "popper.js": "^1.14.7",
    "protobufjs": "^6.8.6",
    "tweetnacl": "^1.0.0",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^7.0.0-beta.49",
    "babel-jest": "^23.0.1",
    "babel-loader": "^8.0.0-beta.4",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-vue": "^4.4.0",
    "express": "^4.16.3",
    "http-proxy-middleware": "^0.19.0",
    "jest": "^23.1.0",
    "json-loader": "^0.5.7",
    "request": "^2.85.0",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.11.0",
    "webpack-auto-inject-version": "^1.1.0",
    "webpack-cli": "^3.0.2",
    "yargs-parser": "^9.0.2"
  }
}
